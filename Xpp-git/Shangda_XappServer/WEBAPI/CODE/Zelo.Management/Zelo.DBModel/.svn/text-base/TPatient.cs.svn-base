using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using Zelo.Common.CustomAttributes;
namespace Zelo.DBModel
{
    [Table(Name = "T_Patient")]
    public class TPatient
    {
        [Id(Name = "PatientID", Strategy = GenerationType.INDENTITY)]
        public int PatientID { get; set; }

        [Column(Name = "PatientMID")]
        public string PatientMID { get; set; }

        [Column(Name = "PatientGID")]
        public string PatientGID { get; set; }

        [Column(Name = "PatientName")]
        public string PatientName { get; set; }


        [Column(Name = "CreateTime")]
        public DateTime CreateTime { get; set; }

        [Column(Name = "UpdateTime")]
        public DateTime? UpdateTime { get; set; }

        [Column(Name = "Avatar")]
        public string Avatar { get; set; }

        [Column(Name = "Phone")]
        public string Phone { get; set; }

        [Column(Name = "Password")]
        public string Password { get; set; }

        [Column(Name = "IDNumber")]
        public string IDNumber { get; set; }

        [Column(Name = "Sex")]
        public int Sex { get; set; }

        [Column(Name = "Age")]
        public int Age { get; set; }

        [Column(Name = "Address")]
        public string Address { get; set; } 

        [Column(Name = "ODisease")]
        public string ODisease { get; set; }

        [Column(Name = "TDisease")]
        public string TDisease { get; set; }

        [Column(Name = "THDisease")]
        public string THDisease { get; set; }

        [Column(Name = "ReservationPhone")]
        public string ReservationPhone { get; set; }

        [Column(Name = "FamilyName")]
        public string FamilyName { get; set; }

        [Column(Name = "FamilyTelephone")]
        public string FamilyTelephone { get; set; }

        [Column(Name = "Height")]
        public int Height { get; set; }

        [Column(Name = "Weight")]
        public int Weight { get; set; }

        [Column(Name = "DiseaseTime")]
        public int? DiseaseTime { get; set; }

        [Column(Name = "DiagnosisTime")]
        public int? DiagnosisTime { get; set; }

        [Column(Name = "IsHighBloodPressure")]
        public int IsHighBloodPressure { get; set; }

        [Column(Name = "HighBloodPressureTime")]
        public int? HighBloodPressureTime { get; set; }

        [Column(Name = "IsDiabetes")]
        public int IsDiabetes { get; set; }

        [Column(Name = "DiabetesTime")]
        public int? DiabetesTime { get; set; }

        [Column(Name = "IsHeartDisease")]
        public int IsHeartDisease { get; set; }

        [Column(Name = "HeartDiseaseTime")]
        public int? HeartDiseaseTime { get; set; }

        [Column(Name = "CapacityForAction")]
        public int CapacityForAction { get; set; }

        [Column(Name = "IsDel")]
        public int IsDel { get; set; }

        [Column(Name = "Status")]
        public int Status { get; set; }
       
        
       

    }
}
